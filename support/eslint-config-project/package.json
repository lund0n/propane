{
  "name": "eslint-config-project",
  "version": "0.0.0",
  "description": "Project ESLint Config",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lund0n/propane.git",
    "directory": "support/eslint-config-project"
  },
  "author": "Jeremy Lund <jeremy.r.lund@gmail.com>",
  "bugs": "https://github.com/lund0n/propane/issues",
  "license": "MIT",
  "keywords": [
    "eslint"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "files": [
    "/*.d.ts",
    "/*.cjs",
    "/*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "format": "prettier --ignore-unknown --write .",
    "format:check": "prettier --cache --ignore-unknown --check .",
    "lint": "eslint --cache .",
    "prepack": "unbuild"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "eslint": "^8.29.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "prettier-config": "workspace:*",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "prettier": "prettier-config"
}
